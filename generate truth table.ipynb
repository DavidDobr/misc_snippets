    "def tablize(n, truths=[]):\n",
    "    if n == 0:\n",
    "        print(truths)\n",
    "    else:\n",
    "        for b in [0, 1]:\n",
    "            tablize(n-1, truths+[b])\n",
    "\n",
    "tablize(4)"
